generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  username String @unique
  password String
}

model Board {
  boardId Int @id @default(autoincrement())
  userId Int
  boardString String
  createdAt DateTime @default(now())
}